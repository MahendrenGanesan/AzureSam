{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DF-FundAccounting"
		},
		"lnk_blob_storage_daily_prices_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lnk_blob_storage_daily_prices'"
		},
		"lnk_stoage_service_gen2_daily_prices_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lnk_stoage_service_gen2_daily_prices'"
		},
		"lnk_stoage_service_gen2_daily_prices_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://assetpricesdl.dfs.core.windows.net/"
		},
		"5PM_Trigger_properties_first_price_flow_parameters_fileName": {
			"type": "string",
			"defaultValue": "testValuetestvalue.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DF-FundAccounting_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lnk_blob_storage_daily_prices_connectionString": {
						"value": "[parameters('lnk_blob_storage_daily_prices_connectionString')]"
					},
					"lnk_stoage_service_gen2_daily_prices_accountKey": {
						"value": "[parameters('lnk_stoage_service_gen2_daily_prices_accountKey')]"
					},
					"lnk_stoage_service_gen2_daily_prices_properties_typeProperties_url": {
						"value": "[parameters('lnk_stoage_service_gen2_daily_prices_properties_typeProperties_url')]"
					},
					"5PM_Trigger_properties_first_price_flow_parameters_fileName": {
						"value": "[parameters('5PM_Trigger_properties_first_price_flow_parameters_fileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}